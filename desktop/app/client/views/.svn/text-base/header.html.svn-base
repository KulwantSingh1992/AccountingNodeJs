<ul class="nav nav-pills pull-left ng-scope" ng-controller="HeaderCtrl">
    <li ng-mouseover="toggleSlideLeft()">
        <img src="images/paxcom-logo-6060.png" title="Paxcom" style="height: 50px;">
    </li>
</ul>

<div class="smallstat">
    <span class="btn-group" style="margin-right: 20px;">
        <span class="btn btn-primary">Today's Orders</span>
    <span ng-if="desktopObject.dashboaddetails.order.STATUS=='NOT_INIT'" class="btn btn-default" style="border: 1px solid #ccc;height: 30px;">
            <img src="images/loader-circle.GIF" alt="loading..." />
        </span>
    <span ng-if="!desktopObject.dashboaddetails.order.STATUS" class="btn btn-default" style="border: 1px solid #ccc;">    
            {{desktopObject.dashboaddetails.order.ordersToday.AMAZON.toString() || '-'}}
        </span>
    </span>
    <span class="btn-group" style="margin-right: 20px;">
        <span class="btn btn-primary">Today's Revenue</span>
    <span ng-if="desktopObject.dashboaddetails.account.STATUS=='NOT_INIT'" class="btn btn-default" style="border: 1px solid #ccc;height: 30px;">
            <img src="images/loader-circle.GIF" alt="loading..." />
        </span>
    <span ng-if="desktopObject.dashboaddetails.account.revenue" class="btn btn-default" style="border: 1px solid #ccc;">
            {{desktopObject.dashboaddetails.account.revenue | currency:"&#8377;" ||'-' }}
        </span>
    </span>
    <span class="btn-group" style="margin-right: 20px;">
        <span class="btn btn-primary">Today's Profit</span>
    <span ng-if="desktopObject.dashboaddetails.account.STATUS=='NOT_INIT'" class="btn btn-default" style="border: 1px solid #ccc;height: 30px;">
            <img src="images/loader-circle.GIF" alt="loading..."/>
        </span>
    <span ng-if="desktopObject.dashboaddetails.account.profit" class="btn btn-default" style="border: 1px solid #ccc;">
            {{desktopObject.dashboaddetails.account.profit | currency:"&#8377;" || '-'}}
        </span>
    </span>
</div>

